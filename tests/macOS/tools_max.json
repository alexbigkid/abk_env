{
  "installed_tools": {
    "pyenv": [
      "find ./unixBin/env -type l -name 'LINK_pyenv.env' -delete",
      "brew uninstall pyenv-virtualenv",
      "brew uninstall pyenv",
      "rm -Rf ~/.pyenv"
    ]
  },
  "installed_apps": {
    "mqttx": [
      "brew uninstall --cask mqttx"
    ],
