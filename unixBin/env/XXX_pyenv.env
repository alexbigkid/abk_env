if [ "$(command -v pyenv)" != "" ]; then
    PrintTrace $TRACE_INFO "  [ðŸŽ¬ Initializing pyenv ...]"
    export PYENV_ROOT="$HOME/.pyenv"
    eval "$(pyenv init --path)"
    eval "$(pyenv init -)"
    PrintTrace $TRACE_INFO "  [ðŸŽ¬ Initializing pyenv virtualenv ...]"
    eval "$(pyenv virtualenv-init -)"

elif [ -d "$HOME/.pyenv/bin" ]; then
    PrintTrace $TRACE_INFO "  [ðŸŽ¬ Initializing pyenv ...]"
    export PYENV_ROOT="$HOME/.pyenv"
    export PATH="$PYENV_ROOT/bin:$PATH"
    eval "$(pyenv init --path)"
    eval "$(pyenv init -)"
    PrintTrace $TRACE_INFO "  [ðŸŽ¬ Initializing pyenv virtualenv ...]"
    eval "$(pyenv virtualenv-init -)"

else
    PrintTrace $TRACE_INFO "  ${YELLOW}pyenv is not installed! Consider managing python installations with pyenv${NC}"
fi

