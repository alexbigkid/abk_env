{
  "anthropic": {
    "apiKey": "${ANTHROPIC_API_KEY}",
    "endpoint": "https://api.anthropic.com/v1",
    "defaultModel": "claude-3.5-sonnet-20241022"
  },
  "openai": {
    "apiKey": "${OPENAI_API_KEY}",
    "endpoint": "https://api.openai.com/v1",
    "defaultModel": "gpt-4"
  },
  "github": {
    "token": "${GITHUB_TOKEN}",
    "api": "https://api.github.com"
  },
  "claude": {
    "maxTokens": 4096,
    "temperature": 0.7,
    "timeout": 30000
  }
}