{
  "defaultSettings": {
    "maxTokens": 4096,
    "temperature": 0.7,
    "topP": 1.0,
    "presencePenalty": 0.0,
    "frequencyPenalty": 0.0,
    "stopSequences": [],
    "timeout": 30000
  },
  "codeGeneration": {
    "preferredLanguages": ["typescript", "python", "bash"],
    "codeStyle": {
      "indentation": "spaces",
      "indentSize": 2,
      "maxLineLength": 100,
      "semicolons": true
    },
    "testing": {
      "framework": "${TESTING_FRAMEWORK}",
      "generateTests": true,
      "testCoverage": true
    }
  },
  "projectPreferences": {
    "defaultFrameworks": {
      "frontend": "${FRONTEND_FRAMEWORK}",
      "backend": "${BACKEND_FRAMEWORK}",
      "database": "${DATABASE_TYPE}",
      "containerization": "docker"
    },
    "gitWorkflow": {
      "branchNaming": "${GIT_BRANCH_PATTERN}",
      "commitConvention": "conventional-commits",
      "autoGenerateChangelogs": true
    }
  },
  "development": {
    "environment": "${DEV_ENVIRONMENT}",
    "debugMode": true,
    "verboseLogging": false,
    "autoSave": true,
    "liveReload": true
  },
  "security": {
    "scanForSecrets": true,
    "enforceSecureDefaults": true,
    "suggestSecurityImprovements": true
  }
}